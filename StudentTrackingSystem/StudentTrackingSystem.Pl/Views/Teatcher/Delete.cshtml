@{
}
@model StudentTrackingSystem.DAL.Models.Teatcher

@{
    ViewData["Title"] = "Delete Teatcher";
}

<h2>Delete Confirmation</h2>

<p>Are you sure you want to delete this teatcher?</p>

<div>
    <strong>Name:</strong> @Model.FullName <br />
    <strong>Subject:</strong> @Model.Subject <br />
</div>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Yes, Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
